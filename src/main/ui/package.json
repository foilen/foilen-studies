{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@jagannathbhat/cra-build-watch": "^1"
  },
  "dependencies": {
    "@popperjs/core": "^2",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^13",
    "@testing-library/user-event": "^14",
    "axios": "^0",
    "bootstrap": "^5",
    "bootstrap-icons": "^1",
    "jquery": "^3",
    "lodash": "^4",
    "luxon": "^2",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6",
    "react-scripts": "^5",
    "react-toastify": "^9",
    "web-vitals": "^3"
  },
  "scripts": {
    "start": "node node_modules/react-scripts/bin/react-scripts start",
    "watch": "cra-build-watch --build-path ../../../build/resources/main/static",
    "build": "node node_modules/react-scripts/bin/react-scripts build",
    "test": "node node_modules/react-scripts/bin/react-scripts test",
    "test:ci": "node node_modules/react-scripts/bin/react-scripts test --watchAll=false --passWithNoTests",
    "eject": "node node_modules/react-scripts/bin/react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
